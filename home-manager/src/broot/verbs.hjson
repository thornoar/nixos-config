verbs: [
    {
        invocation: rm
        external: del {file}
        leave_broot: false
    }
    {
        invocation: clip
        external: "wl-copy -n \"{file}\""
        leave_broot: false
    }
    {
        invocation: srm
        external: rm -rf {file}
        leave_broot: false
    }
    {
        invocation: "call {command}"
        external: "sh -c \"nohup {command} {file} > /dev/null 2>&1 &\""
        leave_broot: false
    }
    {
        invocation: "cmd {command}"
        external: "parsecmd {command} {file}"
        leave_broot: false
    }
    {
        invocation: "acmd {command}"
        external: "parsecmd \"{command} %\" {file}"
        leave_broot: false
    }
    {
        key: ctrl-r
        internal: refresh
    }
    {
        key: ctrl-q
        internal: start_end_panel
    }
    {
        invocation: xs
        cmd: ":parent;:cd"
    }
    {
        invocation: esc
        key: ctrl-c
        internal: escape
        leave_broot: false
    }
    {
        invocation: touch {new_file}
        execution: "touch {directory}/{new_file}"
        leave_broot: false
    }
    {
        invocation: create {subpath}
        shortcut: cr
        execution: "$EDITOR -f {directory}/{subpath}"
        leave_broot: false
    }
    {
        invocation: git_diff
        shortcut: gd
        leave_broot: false
        execution: "git difftool -y {file}"
    }
    {
        invocation: "backup {version}"
        key: ctrl-b
        leave_broot: false
        auto_exec: false
        execution: "cp -r {file} {parent}/{file-stem}-{version}{file-dot-extension}"
    }
    {
        invocation: "cpa {dest}"
        external: "cp -r {directory}/* {dest}"
        from_shell: true
    }
    {
        invocation: mpp
        cmd: ":move_to_panel"
    }
    {
        key: down
        internal: ":line_down_no_cycle 1"
    }
    {
        key: up
        internal: ":line_up_no_cycle 1"
    }
    {
        key: alt-down
        internal: ":line_down_no_cycle 5"
    }
    {
        key: alt-up
        internal: ":line_up_no_cycle 5"
    }
    {
        key: shift-left
        invocation: home
        execution: ":focus ~"
    }
    {
        key: alt-t
        invocation: terminal
        external: "sh -c \"nohup $TERMINAL -e sh -c 'cd \"{directory}\"; $SHELL' > /dev/null 2>&1 &\" > /dev/null 2>&1"
        leave_broot: false
    }
    {
        key: alt-b
        invocation: filemanager
        external: "sh -c \"nohup $TERMINAL -e zsh -c 'cd \"{directory}\"; br; $SHELL' > /dev/null 2>&1 &\" > /dev/null 2>&1"
        leave_broot: false
    }
    {
        key: ctrl-x
        invocation: terminal
        external: "sh -c \"nohup $TERMINAL -e zsh -c 'cd {directory}; nvim-server; br; $SHELL' > /dev/null 2>&1 &\" > /dev/null 2>&1"
        leave_broot: false
    }
    {
        keys: [
            left
            right
        ]
        internal: refresh
    }
    {
        key: alt-left
        internal: back
    }
    {
        invocation: edit
        shortcut: e
        keys: [
            enter
            alt-right
        ]
        apply_to: file
        working_dir: "{root}"
        execution: "$EDITOR -f {file}"
        leave_broot: false
    }
    {
        key: alt-right
        invocation: focus
        apply_to: directory
        internal: focus
    }
    {
        key: alt-enter
        internal: open_stay
    }
    {
        key: shift-right
        apply_to: text_file
        execution: "$EDITOR -f {file} -l {line}"
        leave_broot: false
    }
    {
        key: ctrl-d
        internal: toggle_stage
    }
    {
        key: ctrl-shift-s
        shortcut: sa
        internal: stage_all_files
    }
    {
        invocation: gtr
        execution: ":focus {git-root}"
    }
    {
        key: alt-s
        cmd: ":toggle_stage;:line_down"
    }
    {
        key: ctrl-s
        cmd: ":toggle_hidden"
    }
]
